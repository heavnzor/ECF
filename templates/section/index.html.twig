<!DOCTYPE html>

<title>Section index</title>

{% block body %}
	<h1>Section index</h1>

	<table class="table">
		<thead>
			<tr>
				<th>Id</th>
				<th>Section</th>
				<th>Formation</th>
				<th>Action</th>
			</tr>
		</thead>
		<tbody>
			{% for section in sections %}
				<tr>
					<td>{{ section.id }}</td>
					<td>{{ section.titre }}</td>
					<td>{{ section.formation }}</td>
					<td>
						<a href="{{ path('app_section_show', {'id': section.id}) }}">show</a>
						<a href="{{ path('app_section_edit', {'id': section.id}) }}">edit</a>
					</td>
				</tr>
			{% else %}
				<tr>
					<td colspan="3">no records found</td>
				</tr>
			{% endfor %}
		</tbody>
	</table>

	<a href="{{ path('app_section_new') }}">Create new</a>
{% endblock %}
