				<div class="container ">
					<div class="row">
						<div class="col-xl-9 mx-auto position-relative d-flex justify-content-center">
							<h1 class="mb-5">
								<strong>
									{{ slogan }}
								</strong>
							</h1>
						</div>
						<span class="fs-2 waldganger">Waldganger&trade;</span>
					</div>
				</div>
			</header>
			<div class='container-fluid border border-primary  '>
				<div class='container text-center  justify-content-center px-auto my-3 '>
					{% if is_granted('ROLE_INSTRUCTEUR') %}
						{% include 'navformateurs.html.twig' %}
					{% endif %}
					{% block main %}
						{% for success in app.flashes('success') %}
							<div class="alert alert-success">
								{{ success }}
							</div>
						{% endfor %}
						{% for error in app.flashes('error') %}
							<div class="alert alert-error">
								{{ error }}
							</div>
						{% endfor %}
						<section class="container bg-black justify-content-center mx-auto">
							<div class="row g-3 " id="results">
								{% for formation in formations %}
									<div class="col-md-4 mx-auto d-md-flex flex-column my-3">

										<img src="{{ asset('build/uploads/photos/')}}{{formation.image}}" class=" imgFormation mx-auto" alt=" {{ formation.titre}} ">
										<h2 class="fs-2">{{ formation.titre}}</h2>
										<p class="fs-5">{% set description = formation.description|raw %}
											{{ description|length > 70 ? description|slice(0,70) ~ '...' : description }}</p>
										{% if is_granted('ROLE_USER') %}
											<a href="{{ path('app_formation_show', {'id': formation.id}) }}">
												<button class="btn btn-primary ">En savoir plus</button>
											</a>
										{% endif %}
									</div>
								{% endfor %}
							</div>
						</section>
