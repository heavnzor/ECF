{% extends 'base.html.twig' %}

{% block title %}Progress{% endblock %}

{% block body %}
    <h1>Progress</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ progress.id }}</td>
            </tr>
            <tr>
                <th>FormationFinished</th>
                <td>{{ progress.formationFinished ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>CoursFinished</th>
                <td>{{ progress.coursFinished ? 'Yes' : 'No' }}</td>
            </tr>
